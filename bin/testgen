#!/usr/bin/env ruby

puts "Testgen. Written, owned and distributed by C3 Business Solutions"

begin
  require 'testgen'
rescue LoadError
  $LOAD_PATH.unshift(File.join(File.dirname(__FILE__), '..', 'lib'))
  require 'testgen'
end

db = Database.hash_from_config
ret = TestGen.new().run_test(ARGV.shift,db,CommandRunner.new)

exit(ret)
